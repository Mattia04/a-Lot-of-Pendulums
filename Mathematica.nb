Cell[TextData[{
 "So those are our equations to calculate the movements of the pendulums. We \
can actually write them a bit better by defining:\[LineSeparator]\
\[LineSeparator]",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["c", "TI"], "\[LongEqual]", "cos", 
         RowBox[{"(", 
           RowBox[{"\[Theta]", "-", "\[Phi]"}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "c = \\cos(\\theta-\\phi)", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "40f28ede-0750-4ffd-9bd7-262cec2cbef4"],
 "\[LineSeparator]",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["s", "TI"], "\[LongEqual]", "cos", 
         RowBox[{"(", 
           RowBox[{"\[Theta]", "-", "\[Phi]"}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "s = \\cos(\\theta-\\phi)", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e8281310-aed8-4c6e-905b-92e33ad5485c"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["d", "TI"], "\[LongEqual]", "2", "-", 
         SuperscriptBox[
          StyleBox["c", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "d = 2 - c^2", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ce0bec78-6736-4169-89f7-5f12fd70f813"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["d", "TI"], "2"], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["d", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "d_2 = d^2", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cf6bedfb-fdd4-4c4d-af88-39002a77bde8"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["p", "TI"], "\[LongEqual]", 
         SubscriptBox[
          StyleBox["p", "TI"], "\[Theta]"], 
         SubscriptBox[
          StyleBox["p", "TI"], "\[Phi]"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "p = p_\\theta p_\\phi", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d2b06567-d447-4780-acb4-ebc52334d714"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["n", "TI"], "\[LongEqual]", 
         RowBox[{"(", 
           RowBox[{
             RowBox[{"(", 
               RowBox[{
                 SubsuperscriptBox[
                  StyleBox["p", "TI"], "\[Theta]", "2"], "+", "2", 
                 SubsuperscriptBox[
                  StyleBox["p", "TI"], "\[Phi]", "2"]}], ")"}], 
             StyleBox["c", "TI"], "-", 
             StyleBox["p", "TI"], 
             RowBox[{"(", 
               RowBox[{"2", "+", 
                 SuperscriptBox[
                  StyleBox["c", "TI"], "2"]}], ")"}]}], ")"}], 
         StyleBox["s", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "n = ((p_\\theta^2 + 2 p_\\phi^2 ) c - p (2 + c^2))s", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "13ae96b4-424d-4247-9d01-08475738a44f"],
 "\n\nIn this way we have the four equations:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         OverscriptBox["\[Theta]", "."], "\[LongEqual]", 
         FractionBox[
          RowBox[{"-", 
            SubscriptBox[
             StyleBox["p", "TI"], "\[Theta]"], "+", 
            SubscriptBox[
             StyleBox["p", "TI"], "\[Phi]"], 
            StyleBox["c", "TI"]}], 
          StyleBox["d", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\dot\\theta =\\frac{ - p_\\theta + p_\\phi c}{d}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "c829d7ae-ca23-415a-827d-0f7be83e94b5"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         OverscriptBox["\[Phi]", "."], "\[LongEqual]", 
         FractionBox[
          RowBox[{
            SubscriptBox[
             StyleBox["p", "TI"], "\[Theta]"], "-", "2", 
            SubscriptBox[
             StyleBox["p", "TI"], "\[Phi]"], 
            StyleBox["c", "TI"]}], 
          StyleBox["d", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\dot\\phi = \\frac{  p_\\theta -2 p_\\phi c}{d}", "state" -> "Boxes"|>,
    
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "2389a140-e312-4e83-ae73-b96e9c91700f"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          OverscriptBox[
           StyleBox["p", "TI"], "."], "\[Theta]"], "\[LongEqual]", "-", 
         FractionBox[
          StyleBox["n", "TI"], 
          SubscriptBox[
           StyleBox["d", "TI"], "2"]], "+", "2", 
         StyleBox["g", "TI"], 
         StyleBox["s", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\dot p_\\theta = -\\frac{n}{d_2} + 2 g s", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "2dd25e7b-8b3e-4b2d-8d40-2b5e4a40d5fa"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          OverscriptBox[
           StyleBox["p", "TI"], "."], "\[Phi]"], "\[LongEqual]", 
         FractionBox[
          StyleBox["n", "TI"], 
          SubscriptBox[
           StyleBox["d", "TI"], "2"]], "+", 
         StyleBox["g", "TI"], 
         StyleBox["s", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\dot p_\\phi = \\frac{n}{d_2} + gs", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "45060d75-c4ac-44ec-bf45-460b2acf6044"],
 "\n\nThese equations are ready to be put in our system integrator.\n\n"
}], "Text",
